{
  "name": "airmap",
  "version": "0.0.5",
  "private": true,
  "engines": {
    "node": ">=16"
  },
  "scripts": {
    "clean": "fd -t=d -u node_modules . -x rm -rf {}",
    "dev": "turbo run dev",
    "test": "turbo run test",
    "build": "turbo run build",
    "format": "prettier --write './*.{js,json,md}' './.github/**' '.vscode/**' 'packages/**/*.{md,json,js,ts,jsx,tsx}' 'apps/**/*.{md,json,js,ts,jsx,tsx}'",
    "typecheck": "turbo run typecheck"
  },
  "devDependencies": {
    "@types/geojson": "^7946.0.15",
    "ava": "6.2.0",
    "browser-env": "3.3.0",
    "esbuild": "0.17.7",
    "geojson": "0.5.0",
    "npm-run-all2": "7.0.2",
    "prettier": "^3.4.2",
    "prettier-plugin-organize-imports": "4.1.0",
    "ts-node": "^10.9.2",
    "turbo": "^2.3.3",
    "typescript": "5.7.3"
  },
  "packageManager": "pnpm@9.4.0",
  "ava": {
    "require": [
      "./test/_setup-browser-env.js"
    ],
    "extensions": {
      "ts": "module"
    },
    "nodeArguments": [
      "--loader=ts-node/esm",
      "--experimental-specifier-resolution=node"
    ]
  },
  "workspaces": [
    "packages/*",
    "apps/*"
  ],
  "dependencies": {
    "date-fns": "4.1.0"
  },
  "pnpm": {
    "overrides": {
      "react": "18.3.1",
      "react-dom": "18.3.1"
    }
  }
}
