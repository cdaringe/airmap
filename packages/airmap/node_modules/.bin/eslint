#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/cdaringe/src/airmap/node_modules/.pnpm/eslint@8.6.0/node_modules/eslint/bin/node_modules:/Users/cdaringe/src/airmap/node_modules/.pnpm/eslint@8.6.0/node_modules/eslint/node_modules:/Users/cdaringe/src/airmap/node_modules/.pnpm/eslint@8.6.0/node_modules:/Users/cdaringe/src/airmap/node_modules/.pnpm/node_modules:/Users/cdaringe/src/airmap/node_modules:/Users/cdaringe/src/node_modules:/Users/cdaringe/node_modules:/Users/node_modules:/node_modules:/Users/cdaringe/src/airmap/packages/airmap/node_modules:/Users/cdaringe/src/airmap/packages/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/cdaringe/src/airmap/node_modules/.pnpm/eslint@8.6.0/node_modules/eslint/bin/node_modules:/Users/cdaringe/src/airmap/node_modules/.pnpm/eslint@8.6.0/node_modules/eslint/node_modules:/Users/cdaringe/src/airmap/node_modules/.pnpm/eslint@8.6.0/node_modules:/Users/cdaringe/src/airmap/node_modules/.pnpm/node_modules:/Users/cdaringe/src/airmap/node_modules:/Users/cdaringe/src/node_modules:/Users/cdaringe/node_modules:/Users/node_modules:/node_modules:/Users/cdaringe/src/airmap/packages/airmap/node_modules:/Users/cdaringe/src/airmap/packages/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../node_modules/.pnpm/eslint@8.6.0/node_modules/eslint/bin/eslint.js" "$@"
else
  exec node  "$basedir/../../../../node_modules/.pnpm/eslint@8.6.0/node_modules/eslint/bin/eslint.js" "$@"
fi
